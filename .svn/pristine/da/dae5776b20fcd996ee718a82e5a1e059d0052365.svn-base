$(function(){
	var times=0;
	$("#formhead").click(function(){
		//alert("aaa");
		if(times%2==0){
		$("#formhead1").css({"background": "green"});
		$("#formhead2").css("background-color","white");
		}else{
			$("#formhead1").css({"background": "white"});
			$("#formhead2").css("background-color","green");
		}
		times++;
	});
});

